{
  "hosting": {
    "public": "dist",
    "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "api/send_contact_form",
        "function": "send_contact_form"
      },
      {
        "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }
    
    ]
  }
}
